# Auto detect text files and ensure they use LF line endings on checkout.
* text=auto eol=lf

# Explicitly declare files that will always have LF line endings.
*.py text eol=lf
*.md text eol=lf
*.json text eol=lf
*.xml text eol=lf
*.html text eol=lf
*.css text eol=lf
*.js text eol=lf
*.sh text eol=lf
*.txt text eol=lf
.gitignore text eol=lf
.gitattributes text eol=lf

# Declare files that will always have CRLF line endings (e.g. Windows batch files).
# *.bat text eol=crlf

# Declare files that should be treated as binary and not have their line endings changed.
# *.png binary
# *.jpg binary
# *.jpeg binary
# *.gif binary
# *.bam binary
# *.egg binary
# *.egg.pz binary
# (Panda3D model and texture files should generally be treated as binary)
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.gif filter=lfs diff=lfs merge=lfs -text
*.ttf filter=lfs diff=lfs merge=lfs -text
*.otf filter=lfs diff=lfs merge=lfs -text
*.woff filter=lfs diff=lfs merge=lfs -text
*.woff2 filter=lfs diff=lfs merge=lfs -text
*.bam filter=lfs diff=lfs merge=lfs -text
*.egg filter=lfs diff=lfs merge=lfs -text
*.egg.pz filter=lfs diff=lfs merge=lfs -text
*.obj filter=lfs diff=lfs merge=lfs -text
*.mtl filter=lfs diff=lfs merge=lfs -text
*.gltf filter=lfs diff=lfs merge=lfs -text
*.glb filter=lfs diff=lfs merge=lfs -text
